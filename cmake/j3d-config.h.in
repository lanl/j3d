/* This is a template for a generated header file (r3d-config.h)
 * containing important J3D variables defined at compile time */

#ifndef J3D_CONFIG_H_
#define J3D_CONFIG_H_

// J3D compile time variables
#define J3D_MAX_VERTS @J3D_MAX_VERTS@

#define J3D_MAX_EDGES @J3D_MAX_EDGES@

#define J3D_MAX_ORDER @J3D_MAX_ORDER@

// J2D compile time variables
#define J2D_MAX_VERTS @J2D_MAX_VERTS@

#define J2D_MAX_ORDER @J2D_MAX_ORDER@

// Other compile time variables
#ifndef J3D_ENABLE_Kokkos
#cmakedefine J3D_ENABLE_Kokkos
#endif

#endif
